metrics_prefix: phobos_client

metrics:
  - instrumentation_label: listener.process_message
    bucket: message
  - instrumentation_label: listener.process_batch
    bucket: batch

buckets:
  - name: message
    buckets:
      - 5
      - 10
      - 25
      - 50
      - 100
      - 250
      - 500
      - 750
      - 1000
      - 2500
      - 5000
      - 10000
  - name: batch
    buckets:
      - 100
      - 250
      - 500
      - 750
      - 1000
      - 2500
      - 5000
      - 10000
      - 15000
